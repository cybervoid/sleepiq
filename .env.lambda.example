# Lambda-specific environment variables (example)
SLEEPIQ_USERNAME=your_email@example.com
SLEEPIQ_PASSWORD=your_password

# Basic auth for API Gateway-protected endpoint
API_USERNAME=api_user
API_PASSWORD=api_password

# AWS region for Terraform and SDK
AWS_REGION=us-east-1

# S3 bucket for persistent SleepIQ sessions (must be globally unique)
SESSION_BUCKET_NAME=sleepiq-sessions-unique-name
